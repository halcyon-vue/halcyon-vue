{
  "name": "halcyon-vue",
  "version": "1.1.13",
  "description": "Material 3 library for Vue",
  "author": {
    "name": "Autumn",
    "url": "https://autumns.page"
  },
  "homepage": "https://halcyon-vue.github.io/halcyon-vue/getting-started.html",
  "bugs": "https://github.io/halcyon-vue/halcyon-vue",
  "keywords": [],
  "license": "MIT",
  "main": "./dist/halcyon-vue.cjs",
  "module": "./dist/halcyon-vue.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": "./dist/main.cjs",
      "types": "./dist/index.d.ts"
    },
    "./plugin": {
      "import": "./dist/plugin.js",
      "require": "./dist/plugin.cjs",
      "types": "./dist/plugin.d.ts"
    },
    "./nuxt": {
      "import": "./dist/nuxt.js",
      "require": "./dist/nuxt.cjs",
      "types": "./dist/nuxt.d.ts"
    },
    "./style": "./dist/style.css",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "index.d.ts",
    "src/**/*.d.ts"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "vite build",
    "docs:dev": "vitepress dev docs --host",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@floating-ui/vue": "^1.0.2",
    "@headlessui/vue": "^1.7.16",
    "floating-vue": "2.0.0-beta.24"
  },
  "peerDependencies": {
    "@vueuse/components": "^10.2.0",
    "@vueuse/core": "^10.2.0",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@algolia/client-search": "^4.19.1",
    "@iconify-json/mdi": "^1.1.54",
    "@nuxt/kit": "^3.7.0",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "eslint": "^8.48.0",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.0.2",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "nanoid": "^4.0.2",
    "sass": "^1.66.1",
    "typescript": "*",
    "unplugin-icons": "^0.16.6",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.5.3",
    "vitepress": "^1.0.0-rc.10",
    "vue-tsc": "^1.8.8"
  }
}